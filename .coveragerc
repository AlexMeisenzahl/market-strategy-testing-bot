[run]
source = .
omit =
    */tests/*
    */test_*.py
    */migrations/*
    */venv/*
    */.venv/*
    */site-packages/*
    */demo*.py
    */start_*.py
    */QUICK_START*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

precision = 2
show_missing = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
